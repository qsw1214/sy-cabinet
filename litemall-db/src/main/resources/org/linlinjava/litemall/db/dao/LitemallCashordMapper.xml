 <!-- 查询体现记录 关联用户表 银行卡表 管理员表 -->
  <select id="CashordList" resultMap="BaseResultMapUnion" parameterType="map" >
  	select cas.id,cas.money,cas.sub_time,cas.`status`,bank.user_name,
  	bank.user_phone,bank.card_num,cas.remarks,cas.user_id,
  	cas.up_time,bank.own_bank,
  	admin.username from litemall_cashord as cas 
  	left join litemall_user as us on cas.user_id = us.id 
  	left join litemall_bank_card as bank on cas.bank_id = bank.id
	left join litemall_admin as admin on cas.admin_user_id = admin.id 
	where bank.user_name like #{username,jdbcType=VARCHAR}
	<if test="status != -1">
	and cas.`status` = #{status,jdbcType=INTEGER}
	</if> 
	and cas.delete = 0  ORDER BY cas.id desc 
	limit #{page,jdbcType=INTEGER},#{size,jdbcType=INTEGER}
  </select>
 
